{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/__legacy/(.*)", "destination": "/__legacy/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/__legacy/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
