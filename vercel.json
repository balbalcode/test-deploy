{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/harbor/__legacy/(.*)", "destination": "/harbor/__legacy/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/harbor/__legacy/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ]
}
